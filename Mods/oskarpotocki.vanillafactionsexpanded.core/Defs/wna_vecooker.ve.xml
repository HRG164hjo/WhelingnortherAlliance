<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="WNA_FurnitureBase">
		<defName>WNA_VECooker</defName>
		<label>processing tank</label>
		<description>A small container but is capable of holding more than the volume it seems. Can be used to process various raw materials.</description>
		<tickerType>Rare</tickerType>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>buildings/misc/wna_cooker</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(255,255,255,200)</color>
		</graphicData>
		<size>(1,1)</size>
		<passability>PassThroughOnly</passability>
        <defaultPlacingRot>South</defaultPlacingRot>
        <researchPrerequisites Inherit="false">
            <li>WNA_MatterTransformation</li>
        </researchPrerequisites>
		<inspectorTabs>
			<li>PipeSystem.ITab_Processor</li>
		</inspectorTabs>
		<comps>
			<li Class="PipeSystem.CompProperties_AdvancedResourceProcessor">
				<notWorkingKey>WNA_KEY_Facility_Not_Working</notWorkingKey>
				<progressBarOffset>(0,0,-0.75)</progressBarOffset>
				<hideSettings>true</hideSettings>
				<processes>
					<li>WNA-Hay-Chemfuel</li>
					<li>WNA-PsychoidLeaves-Yayo</li>
					<li>WNA-SmokeleafLeaves-SmokeleafJoint</li>
					<li>WNA-WoodLog-Neutroamine</li>
					<li>WNA-Wort-Beer</li>

					<li MayRequire="sarg.rimbees">WNA-RB-Must-Mead</li>

					<li MayRequire="romyashi.perfumes">WNA-Romy-AromaflowerPetals-AncientPerfume</li>

					<li MayRequire="tidal.grainsandbrewing">WNA-GAB-BaijuMash-Baiju</li>
					<li MayRequire="tidal.grainsandbrewing">WNA-GAB-Ethanol-Ether</li>
					<li MayRequire="tidal.grainsandbrewing">WNA-GAB-RaksiMash-Raksi</li>
					<li MayRequire="tidal.grainsandbrewing">WNA-GAB-UnfermentedKvass-Kvass</li>
					<li MayRequire="tidal.grainsandbrewing">WNA-GAB_VinegarMust-Vinegar</li>
					
					<li MayRequire="sirmashedpotato.ashlands">WNA-Mashed-AlcoholShein-DagothBrandy</li>
					<li MayRequire="sirmashedpotato.ashlands">WNA-Mashed-ComberryPrecursor-AlcoholShein</li>
					<li MayRequire="sirmashedpotato.ashlands">WNA-Mashed-MeadowRyePrecursor-AlcoholFlin</li>
					<li MayRequire="sirmashedpotato.ashlands">WNA-Mashed-SaltricePrecursor-AlcoholMazte</li>
					<li MayRequire="sirmashedpotato.ashlands">WNA-Mashed-WickwheatPrecursor-AlcoholSujamma</li>
				</processes>
			</li>
		</comps>
	</ThingDef>

	<!-- recipes -->
	<PipeSystem.ProcessDef Name="WNA_VECookerBillsMain" Abstract="true">
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<autoExtract>true</autoExtract>
		<useIngredients>true</useIngredients>
		<transfersIngredientList>true</transfersIngredientList>
		<priorityInBillList>0</priorityInBillList>
		<noPowerDestroysProgress>false</noPowerDestroysProgress>
		<noPowerDestroysMessage>WNA_KEY_Facility_Need_Power</noPowerDestroysMessage>
		<noPowerDestroysInitialWarning>WNA_KEY_Facility_Process_Failure</noPowerDestroysInitialWarning>
		<rareTicksToDestroy>250</rareTicksToDestroy>
		<destroyIngredientsOnStart>true</destroyIngredientsOnStart>
	</PipeSystem.ProcessDef>
	<PipeSystem.ProcessDef ParentName="WNA_VECookerBillsMain">
		<defName>WNA-Hay-Chemfuel</defName>
		<label>Process hay for chemfuel</label>
		<description>Turn 75 hay into 25 chemfuel.</description>
		<ingredients>
			<li>
				<thing>Hay</thing>
				<countNeeded>75</countNeeded>
			</li>
		</ingredients>
		<results>
			<li>
				<thing>Chemfuel</thing>
				<count>25</count>
			</li>
		</results>
		<ticks>60000</ticks>
	</PipeSystem.ProcessDef>
