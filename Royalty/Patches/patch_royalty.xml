<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/TraderKindDef[defName="WNA_Trader_Settlement" or defName="WNA_Trader_Orbital"]/stockGenerators</xpath>
        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>PsychicAmplifier</thingDef>
                <price>VeryCheap</price>
                <countRange>20000~30000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>BroadshieldCore</thingDef>
                <price>VeryCheap</price>
                <countRange>20000~30000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AnimusStone</thingDef>
                <price>VeryCheap</price>
                <countRange>2000~3000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>WNA_SeedAnima</thingDef>
                <price>VeryCheap</price>
                <countRange>10000~15000</countRange>
            </li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Piano"]/building</xpath>
        <value>
			<paintable>true</paintable>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Drape"]</xpath>
        <value>
			<comps>
                <li>
                    <compClass>CompQuality</compClass>
                </li>
            </comps>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[@Name="ThroneBase"]/comps</xpath>
        <value>
            <li>
                <compClass>CompQuality</compClass>
            </li>
        </value>
    </Operation>

</Patch>