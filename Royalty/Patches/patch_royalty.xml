<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/TraderKindDef[@Name="WNA_TraderKindBase"]/stockGenerators</xpath>
        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>PsychicAmplifier</thingDef>
                <price>VeryCheap</price>
                <countRange>200000~300000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>BroadshieldCore</thingDef>
                <price>VeryCheap</price>
                <countRange>200000~300000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AnimusStone</thingDef>
                <price>VeryCheap</price>
                <countRange>20000~30000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef" MayRequire="regrowth.botr.core">
                <thingDef>RG_AnimaSeed</thingDef>
                <price>VeryCheap</price>
                <countRange>100000~200000</countRange>
            </li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Piano"]/building</xpath>
        <value>
			<paintable>true</paintable>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Drape" or defName="Brazier"]</xpath>
        <value>
			<comps>
                <li>
                    <compClass>CompQuality</compClass>
                </li>
            </comps>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/ThingDef[defName="Brazier"]/stuffCategories/li[text()="Woody"]</xpath>
		<nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="Brazier"]/stuffCategories</xpath>
            <value>
                <li>Woody</li>
            </value>
        </nomatch>
    </Operation>

    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[@Name="ThroneBase"]/comps</xpath>
        <value>
            <li>
                <compClass>CompQuality</compClass>
            </li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Apparel_Coronet" or defName="Apparel_Crown" or defName="Apparel_PsyfocusHelmet" or defName="Apparel_EltexSkullcap" or defName="Apparel_HatLadies" or defName="Apparel_HatTop" or defName="Apparel_CrownStellic"]/apparel</xpath>
        <value>
            <renderSkipFlags>
                <li>None</li>
            </renderSkipFlags>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThoughtDef[defName="AteFoodInappropriateForTitle" or defName="AnimaScream"]</xpath>
        <value>
            <nullifyingPrecepts MayRequire="ludeon.rimworld.ideology">
                <li>WNA_P_Proselyte</li>
            </nullifyingPrecepts>
        </value>
    </Operation>
</Patch>