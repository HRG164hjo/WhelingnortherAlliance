            <RageVirusEnvironmentResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</RageVirusEnvironmentResistance>
            
            <NecrotionResistance MayRequireAnyOf="blackmarket420.taofc,blackmarket420.taofc.1.5">10</NecrotionResistance>
            <NecrotionEnvironmentResistance MayRequireAnyOf="blackmarket420.taofc,blackmarket420.taofc.1.5">10</NecrotionEnvironmentResistance>

            <Radiation MayRequire="dubwise.rimatomics">0</Radiation>
            
