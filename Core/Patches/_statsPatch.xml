        <xpath>Defs/ThingDef[defName="WNA_WNThan" or defName="WNA_Human" or @Name="WNA_AnimalRaceBase"]/statBases</xpath>
        <value>
            <Mashed_Ashlands_AshResistance MayRequire="sirmashedpotato.ashlands">10</Mashed_Ashlands_AshResistance>
            
            <HeatNullNaniteResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</HeatNullNaniteResistance>
            <HeatNullNaniteEnvironmentResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</HeatNullNaniteEnvironmentResistance>
            <ColdNullNaniteResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</ColdNullNaniteResistance>
            <ColdNullNaniteEnvironmentResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</ColdNullNaniteEnvironmentResistance>
            <RadiationResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</RadiationResistance>
            <RadiationEnvironmentResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</RadiationEnvironmentResistance>
            <AntiOrgToxinResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</AntiOrgToxinResistance>
            <AntiOrgToxinEnvironmentResistance MayRequireAnyOf="blackmarket420.kraltechindustries,blackmarket420.kraltechindustrieslite">10</AntiOrgToxinEnvironmentResistance>
