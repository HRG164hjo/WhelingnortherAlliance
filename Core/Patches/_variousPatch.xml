    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ResearchProjectDef[defName!="BasicMechtech"]/heldByFactionCategoryTags</xpath>
        <value>
            <li>WNA_FactionWNA</li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/StatDef[@Name="ArmorRatingBase"]/maxValue</xpath>
        <match Class="PatchOperationRemove">
            <xpath>Defs/StatDef[@Name="ArmorRatingBase"]/maxValue</xpath>
        </match>
    </Operation>

    <Operation Class="PatchOperationConditional">
        <xpath>Defs/FactionDef/autoFlee</xpath>
        <nomatch Class="PatchOperationAdd">
            <xpath>Defs/FactionDef</xpath>
            <value>
                <autoFlee>false</autoFlee>
            </value>
        </nomatch>
        <match Class="PatchOperationReplace">
            <xpath>Defs/FactionDef/autoFlee</xpath>
            <value>
                <autoFlee>false</autoFlee>
            </value>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="ChunkMechanoidSlag"]</xpath>
        <value>
            <smeltable>true</smeltable>
            <smeltProducts>
                <Steel>15</Steel>
                <Plasteel>5</Plasteel>
                <Silver>10</Silver>
            </smeltProducts>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationRemove">
        <xpath>Defs/ThingDef[@ParentName="MechSerumBase"]/tradeability</xpath>
    </Operation>

