    <RecipeDef ParentName="WNA_RecipeBase" MayRequire="sarg.rimbees">
        <defName>WNA_RiteAbletingMult</defName>
        <description>break down slag chunks to retrieve useful things, more productive than traditional methods.</description>
        <jobString>working.</jobString>
        <label>rite ableting x50</label>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Chunks</li>
                    </categories>
                </filter>
                <count>50</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>Chunks</li>
            </categories>
