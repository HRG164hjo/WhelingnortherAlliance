<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <Operation Class="PatchOperationFindMod">
	    <mods>
            <li>Stone Bollards</li>
	    </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="Bollard" or defName="GrandBollard" or defName="ReinforcedBollard"]/stuffCategories</xpath>
		            <value>
		            	<li>Woody</li>
		            </value>
				</li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="HissingCatBollard"]/stuffCategories</xpath>
		            <value>
		            	<li>Fabric</li>
		            </value>
				</li>
            </operations>
	    </match>
    </Operation>
    
</Patch>