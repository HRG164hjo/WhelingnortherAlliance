<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationReplace">
	    <xpath>Defs/BiomeDef/maxFishPopulation</xpath>
        <value>
            <maxFishPopulation>6000000000</maxFishPopulation>
        </value>
    </Operation>

    <!-- -->
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/DamageDef[defName="BombSuper"]</xpath>
        <value>
            <additionalHediffs>
                <li>
                    <hediff>WNA_RadEffect</hediff>
                    <severityPerDamageDealt>0.01</severityPerDamageDealt>
                </li>
            </additionalHediffs>
        </value>
    </Operation>
    
    <!-- -->
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/FactionDef[defName!="WNA_FactionWNA"]/disallowedMemes</xpath>
        <match Class="PatchOperationAdd">
            <xpath>Defs/FactionDef[defName!="WNA_FactionWNA"]/disallowedMemes</xpath>
            <value>
                <li MayRequire="Ludeon.RimWorld.Ideology">Meme_WNA_Ideo</li>
            </value>
        </match>
    </Operation>
    
    <!-- -->
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/HediffDef[defName="CatatonicBreakdown"]/stages/li[1]/capMods</xpath>
        <value>
            <li>
                <capacity>Moving</capacity>
                <postFactor>0</postFactor>
            </li>
            <li>
                <capacity>Manipulation</capacity>
                <postFactor>0</postFactor>
            </li>
        </value>
    </Operation>

    <!-- -->
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/HediffGiverSetDef[defName="Human"]/hediffGivers</xpath>
        <value>
            <li Class="WNA.WNAMiscs.HedifferRemover"/>
        </value>
    </Operation>

    <!-- -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/InventoryStockGroupDef[defName="Medicine"]/thingDefs</xpath>
		<value>
			<li>WNA_Medikit</li>
		</value>
	</Operation>

    <!-- -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="Make_ChemfuelFromWood"]</xpath>
		<value>
			<allowMixingIngredients>true</allowMixingIngredients>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="Make_ChemfuelFromWood"]/ingredients/li[1]/filter/thingDefs</xpath>
		<value>
            <li MayRequire="sarg.alphabiomes">GU_RedWood</li>
            <li MayRequire="sarg.alphabiomes">AB_CrystalWood</li>
            <li MayRequire="sarg.alphabiomes">AB_MushroomWoodLog</li>
            <li MayRequire="biomesteam.biomescaverns">BMT_MushroomLog</li>
            <li MayRequire="sirmashedpotato.ashlands">Mashed_Ashlands_ParasolWoodLog</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Cork</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rattan</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_ReedStalks</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_EbonyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_MahoganyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_RosewoodWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_CedarWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rubber</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="Make_ChemfuelFromWood"]/fixedIngredientFilter/thingDefs</xpath>
		<value>
            <li MayRequire="sarg.alphabiomes">GU_RedWood</li>
            <li MayRequire="sarg.alphabiomes">AB_CrystalWood</li>
            <li MayRequire="sarg.alphabiomes">AB_MushroomWoodLog</li>
            <li MayRequire="biomesteam.biomescaverns">BMT_MushroomLog</li>
            <li MayRequire="sirmashedpotato.ashlands">Mashed_Ashlands_ParasolWoodLog</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Cork</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rattan</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_ReedStalks</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_EbonyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_MahoganyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_RosewoodWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_CedarWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rubber</li>
		</value>
	</Operation>
    
    <!-- -->
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/TerrainDef/changeable</xpath>
    </Operation>
    
    <!-- -->
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/ThingDef/intricate</xpath>
        <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef/intricate</xpath>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/ThingDef/tradeNeverStack</xpath>
        <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef/tradeNeverStack</xpath>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef/plant/interferesWithRoof</xpath>
    </Operation>
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef/plant/blockAdjacentSow</xpath>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Plant_Agave"]/plant</xpath>
        <value>
            <sowTags>
                <li>Ground</li>
                <li>Hydroponic</li>
            </sowTags>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef[defName="DeepDrill"]/placeWorkers/li[text()="PlaceWorker_InvalidOverSubstructure"]</xpath>
    </Operation>
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef[defName="GroundPenetratingScanner"]/placeWorkers/li[text()="PlaceWorker_NotUnderRoof"]</xpath>
    </Operation>
    
    <Operation Class="PatchOperationAddModExtension">
	    <xpath>Defs/ThingDef[defName="Human"]</xpath>
        <value>
            <li Class="WNA.DMExtension.MassCapacity">
				<massCapacity>150</massCapacity>
			</li>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Bullet_Shell_AntigrainWarhead"]</xpath>
        <value>
            <comps>
                <li Class="WNA.ThingCompProp.CompRadFieldBurst">
                    <config>
                        <radLevel>55</radLevel>
                        <radius>9.9</radius>
                    </config>
                </li>
            </comps>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef/comps/li[@Class="CompProperties_Stunnable"]/affectedDamageDefs</xpath>
        <value>
            <li>WNA_RadBurn</li>
        </value>
    </Operation>
    
    <!-- -->
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThoughtDef[defName="Expectations" or defName="AteRawFood" or defName="AteKibble" or defName="AteCorpse" or defName="AteHumanlikeMeatDirect" or defName="AteHumanlikeMeatAsIngredient" or defName="AteInsectMeatDirect" or defName="AteInsectMeatAsIngredient" or defName="AteRottenFood" or defName="AteWithoutTable" or defName="SleepDisturbed" or defName="KnowPrisonerSold" or defName="KnowGuestOrganHarvested" or defName="ButcheredHumanlikeCorpse" or defName="KnowButcheredHumanlikeCorpse" or defName="ButcheredHumanlikeCorpseOpinion" or defName="ObservedLayingCorpse" or defName="WitnessedDeathNonAlly" or defName="DeniedJoining" or defName="PrisonerBanishedToDie" or defName="InsultedMood" or defName="EnvironmentDark" or defName="WrongApparelGender" or defName="ApparelDamaged" or defName="DeadMansApparel" or defName="HumanLeatherApparelSad" or defName="HumanLeatherApparelHappy" or defName="NeedJoy" or defName="NeedComfort" or defName="NeedBeauty" or defName="NeedRoomSize" or defName="NeedOutdoors" or defName="NeedIndoors" or defName="PrisonCell" or defName="PrisonBarracks" or defName="HospitalPatientRoomStats" or defName="SoldPrisoner" or defName="ExecutedPrisoner" or defName="SharedBed" or defName="Pain" or defName="PsychicDrone" or defName="BrawlerUnhappy" or defName="Ascetic" or defName="Greedy" or defName="Jealous" or defName="SleptInBarracks" or defName="SleptInBedroom" or defName="KnowGuestExecuted" or defName="KnowColonistExecuted"]</xpath>
        <value>
            <nullifyingPrecepts MayRequire="ludeon.rimworld.ideology">
                <li>WNA_P_Proselyte</li>
            </nullifyingPrecepts>
        </value>
    </Operation>
</Patch>