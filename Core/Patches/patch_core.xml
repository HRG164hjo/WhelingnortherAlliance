<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/HediffGiverSetDef[defName="Human"]/hediffGivers</xpath>
        <value>
            <li Class="WNA.WNAMiscs.HedifferRemover">
                <tickInterval>600</tickInterval>
            </li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/FactionDef[defName!="WNA_FactionWNA"]/disallowedMemes</xpath>
        <match Class="PatchOperationAdd">
            <xpath>Defs/FactionDef[defName!="WNA_FactionWNA"]/disallowedMemes</xpath>
            <value>
                <li MayRequire="Ludeon.RimWorld.Ideology">Meme_WNA_Ideo</li>
            </value>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/ThingDef/intricate</xpath>
        <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef/intricate</xpath>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationConditional">
	    <xpath>Defs/ThingDef/tradeNeverStack</xpath>
        <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef/tradeNeverStack</xpath>
        </match>
    </Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/InventoryStockGroupDef[defName="Medicine"]/thingDefs</xpath>
		<value>
			<li>WNA_Medikit</li>
		</value>
	</Operation>
    
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="Make_ChemfuelFromWood"]</xpath>
		<value>
			<allowMixingIngredients>true</allowMixingIngredients>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="Make_ChemfuelFromWood"]/ingredients/li[1]/filter/thingDefs</xpath>
		<value>
            <li MayRequire="sarg.alphabiomes">GU_RedWood</li>
            <li MayRequire="sarg.alphabiomes">AB_CrystalWood</li>
            <li MayRequire="sarg.alphabiomes">AB_MushroomWoodLog</li>
            <li MayRequire="biomesteam.biomescaverns">BMT_MushroomLog</li>
            <li MayRequire="sirmashedpotato.ashlands">Mashed_Ashlands_ParasolWoodLog</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Cork</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rattan</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_ReedStalks</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_EbonyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_MahoganyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_RosewoodWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_CedarWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rubber</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="Make_ChemfuelFromWood"]/fixedIngredientFilter/thingDefs</xpath>
		<value>
            <li MayRequire="sarg.alphabiomes">GU_RedWood</li>
            <li MayRequire="sarg.alphabiomes">AB_CrystalWood</li>
            <li MayRequire="sarg.alphabiomes">AB_MushroomWoodLog</li>
            <li MayRequire="biomesteam.biomescaverns">BMT_MushroomLog</li>
            <li MayRequire="sirmashedpotato.ashlands">Mashed_Ashlands_ParasolWoodLog</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Cork</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rattan</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_ReedStalks</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_EbonyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_MahoganyWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_RosewoodWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_CedarWood</li>
            <li MayRequire="VVenchov.VVNewHarvest">VV_Rubber</li>
		</value>
	</Operation>
    
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/TerrainDef/changeable</xpath>
    </Operation>
    
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef/plant/interferesWithRoof</xpath>
    </Operation>
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef/plant/blockAdjacentSow</xpath>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Plant_Agave"]/plant</xpath>
        <value>
            <sowTags>
                <li>Ground</li>
                <li>Hydroponic</li>
            </sowTags>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef[defName="DeepDrill"]/placeWorkers/li[text()="PlaceWorker_InvalidOverSubstructure"]</xpath>
    </Operation>
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/ThingDef[defName="GroundPenetratingScanner"]/placeWorkers/li[text()="PlaceWorker_NotUnderRoof"]</xpath>
    </Operation>
    
    <Operation Class="PatchOperationAddModExtension">
	    <xpath>Defs/ThingDef[defName="Human"]</xpath>
        <value>
            <li Class="WNA.DMExtension.MassCapacity">
				<massCapacity>150</massCapacity>
			</li>
        </value>
    </Operation>
    
</Patch>