    <ThingDef ParentName="WNA_BackpackBase">
        <defName>WNA_Toolkit</defName>
        <label>tool kit</label>
        <description>A pack containing multiple tools for complex tasks under various circumstances, from simple repairing and mending to emergent medical assistance.</description>
        <graphicData>
            <texPath>apparel/wna_utility</texPath>
        </graphicData>
		<tradeability>None</tradeability>
        <statBases>
            <MarketValue>2000</MarketValue>
			<Mass>0.5</Mass>
        </statBases>
	    <costList>
	        <WNA_AttunementFabric>30</WNA_AttunementFabric>
            <ComponentIndustrial>4</ComponentIndustrial>
	    </costList>
        <equippedStatOffsets>
            <CarryingCapacity>100000</CarryingCapacity>
            <ForagedNutritionPerDay>5</ForagedNutritionPerDay>
            <WorkSpeedGlobal>5</WorkSpeedGlobal>
            <GeneralLaborSpeed>5</GeneralLaborSpeed>
            <ConstructSuccessChance>1</ConstructSuccessChance>
            <FixBrokenDownBuildingSuccessChance>1</FixBrokenDownBuildingSuccessChance>
            <MedicalSurgerySuccessChance>1</MedicalSurgerySuccessChance>
            <CleaningSpeed>5</CleaningSpeed>
            <MedicalTendSpeed>5</MedicalTendSpeed>
            <MedicalOperationSpeed>5</MedicalOperationSpeed>
            <MedicalTendQuality>5</MedicalTendQuality>
        </equippedStatOffsets>
        <recipeMaker>
            <researchPrerequisite>WNA_RebelTools</researchPrerequisite>
            <unfinishedThingDef>WNA_UnfinishedThing</unfinishedThingDef>
            <workSkill>Crafting</workSkill>
            <skillRequirements>
                <Crafting>5</Crafting>
            </skillRequirements>
            <recipeUsers>
                <li>TableMachining</li>
            </recipeUsers>
        </recipeMaker>
    </ThingDef>
	
