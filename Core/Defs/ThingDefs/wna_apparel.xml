                <li>FullHead</li>
                <li>Neck</li>
            </bodyPartGroups>
            <wornGraphicPath>apparel/advflakhead/advflakhead</wornGraphicPath>
            <layers>
                <li>Overhead</li>
                <li>EyeCover</li>
            </layers>
            <renderSkipFlags>
                <li>None</li>
            </renderSkipFlags>
            <blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
        </apparel>
        <recipeMaker>
