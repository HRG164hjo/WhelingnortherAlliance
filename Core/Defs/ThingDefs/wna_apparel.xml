    <ThingDef ParentName="WNA_ApparelBaseA">
        <defName>WNA_AllianceHood</defName>
        <label>alliance hood</label>
        <description>Upper part of WNA standard equipment. This hood covers their eyes, symbolizing one of the WNA's beliefs - don't be blinded by radiance.</description>
        <graphicData>
            <texPath>apparel/alliancehood/icon_alliancehood</texPath>
        </graphicData>
	    <costList>
	        <WNA_Focus>20</WNA_Focus>
	        <WNA_DarkAsbestos>40</WNA_DarkAsbestos>
	        <Silver>20</Silver>
	    </costList>
        <statBases>
            <MarketValue>2000</MarketValue>
            <ArmorRating_Sharp>2000</ArmorRating_Sharp>
            <ArmorRating_Blunt>2000</ArmorRating_Blunt>
            <ArmorRating_Heat>2000</ArmorRating_Heat>
        </statBases>
        <equippedStatOffsets>
            <SuppressionPower>2000</SuppressionPower>
            <ShootingAccuracyPawn>20</ShootingAccuracyPawn>
            <RangedCooldownFactor>-20</RangedCooldownFactor>
            <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
            <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>
                <li>FullHead</li>
                <li>Neck</li>
            </bodyPartGroups>
            <wornGraphicPath>apparel/alliancehood/alliancehood</wornGraphicPath>
            <layers>
                <li>Overhead</li>
                <li>EyeCover</li>
            </layers>
            <blocksAddedPartWoundGraphics>true</blocksAddedPartWoundGraphics>
        </apparel>
        <modExtensions/>
    </ThingDef>
    <ThingDef ParentName="WNA_ApparelBaseA">
		<defName>WNA_AllianceCloak</defName>
		<label>alliance cloak</label>
		<description>Outer layer of WNA standard equipment. This cloak covers almost all of their body, symbolizing one of WNA's thoughts - abnegating what they used to have and to be, to approach transcendence, divinity and salvation.</description>
		<graphicData>
			<texPath>apparel/alliancecloak/icon_alliancecloak</texPath>
		</graphicData>
	    <costList>
	        <WNA_Focus>40</WNA_Focus>
	        <WNA_DarkAsbestos>80</WNA_DarkAsbestos>
	        <Silver>30</Silver>
	    </costList>
        <statBases>
            <MarketValue>2000</MarketValue>
            <ArmorRating_Sharp>2000</ArmorRating_Sharp>
            <ArmorRating_Blunt>2000</ArmorRating_Blunt>
            <ArmorRating_Heat>2000</ArmorRating_Heat>
        </statBases>
        <equippedStatOffsets>
            <SuppressionPower>2000</SuppressionPower>
            <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
            <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li>Hands</li>
				<li>Feet</li>
            </bodyPartGroups>
            <wornGraphicPath>apparel/alliancecloak/alliancecloak</wornGraphicPath>
            <layers>
                <li>Shell</li>
            </layers>
        </apparel>
        <modExtensions/>
    </ThingDef>
    <ThingDef ParentName="WNA_ApparelBaseA">
		<defName>WNA_AllianceRobe</defName>
		<label>alliance robe</label>
		<description>Inner layer of WNA standard equipment. This robe is durable, reliable and comfortable, exhibiting WNA's good treatment of their own people and their strong logistics scheduling capabilities to some extent.</description>
		<graphicData>
			<texPath>apparel/alliancerobe/icon_alliancerobe</texPath>
            <color>(43,43,43)</color>
		</graphicData>
	    <costList>
	        <WNA_Focus>30</WNA_Focus>
	        <WNA_DarkAsbestos>60</WNA_DarkAsbestos>
	        <Silver>20</Silver>
	    </costList>
        <statBases>
            <MarketValue>2000</MarketValue>
            <ArmorRating_Sharp>2000</ArmorRating_Sharp>
            <ArmorRating_Blunt>2000</ArmorRating_Blunt>
            <ArmorRating_Heat>2000</ArmorRating_Heat>
            <Insulation_Cold>2000</Insulation_Cold>
            <Insulation_Heat>2000</Insulation_Heat>
        </statBases>
        <equippedStatOffsets>
            <MeleeDodgeChance>10</MeleeDodgeChance>
            <ImmunityGainSpeed>10</ImmunityGainSpeed>
            <InjuryHealingFactor>10</InjuryHealingFactor>
            <CarryingCapacity>100000</CarryingCapacity>
            <ArmorRating_Sharp>2000</ArmorRating_Sharp>
            <ArmorRating_Blunt>2000</ArmorRating_Blunt>
            <ArmorRating_Heat>2000</ArmorRating_Heat>
            <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
            <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li>Hands</li>
				<li>Feet</li>
            </bodyPartGroups>
            <wornGraphicPath>apparel/alliancerobe/alliancerobe</wornGraphicPath>
            <layers>
                <li>OnSkin</li>
                <li>Middle</li>
            </layers>
        </apparel>
        <comps>
            <li Class="WNA.ThingCompProp.CompApparelEquipHediff">
                <hediff>WNA_RobeBoost</hediff>
                <checkInterval>50</checkInterval>
            </li>
            <li Class="WNA.ThingCompProp.CompApparelHediffRemover">
                <interval>50</interval>
                <removeInjury>true</removeInjury>
            </li>
        </comps>
        <modExtensions/>
    </ThingDef>

