<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="WNA_ItemsBase" MayRequire="ludeon.rimworld.royalty">
        <defName>WNA_SeedAnima</defName>
        <label>anima tree seed</label>
        <description>An egg-like soft jelly ball placed on a small maintaining plate. When planted at somewhere available, this ball of biomass will activate and grow into an anima tree.</description>
        <graphicData>
            <texPath>items/wna_treeseed/wna_treeseed</texPath><!--wna_seedanima-->
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <drawSize>0.8</drawSize>
            <color>(153,185,170)</color>
        </graphicData>
        <statBases>
            <MarketValue>10</MarketValue>
        </statBases>
        <comps>
            <li Class="CompProperties_Plantable">
                <plantDefToSpawn>Plant_TreeAnima</plantDefToSpawn>
            </li>
        </comps>
    </ThingDef>

