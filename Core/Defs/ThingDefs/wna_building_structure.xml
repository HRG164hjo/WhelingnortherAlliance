    <ThingDef ParentName="WNA_StructureBase">
		<defName>WNA_FortressWall</defName>
		<label>fortress wall</label>
		<description>An advanced concrete wall, used for fortresses. What's special about this wall is that it can restore itself due to how it is built.</description>
    	<replaceTags>
    	  	<li>Wall</li>
    	</replaceTags>
		<graphicData>
			<texPath>buildings/atlas/wna_cementwall_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(94,94,94)</color>
			<linkType>CornerFiller</linkType>
			<linkFlags>
				<li>Wall</li>
				<li>Rock</li>
			</linkFlags>
		</graphicData>
    	<drawStyleCategory>Walls</drawStyleCategory>
		<altitudeLayer>Building</altitudeLayer>
		<uiIconPath>buildings/atlas/wna_cementwall_icon</uiIconPath>
        <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<useHitPoints>true</useHitPoints>
		<coversFloor>true</coversFloor>
    	<noRightClickDraftAttack>true</noRightClickDraftAttack>
		<statBases>
			<MaxHitPoints>60000</MaxHitPoints>
			<WorkToBuild>390</WorkToBuild>
			<IncomingDamageFactor>0.01</IncomingDamageFactor>
		</statBases>
		<building>
            <isAirtight>true</isAirtight>
			<isInert>true</isInert>
            <isPlaceOverableWall>true</isPlaceOverableWall>
			<canPlaceOverWall>true</canPlaceOverWall>
            <supportsWallAttachments>true</supportsWallAttachments>
			<ai_chillDestination>false</ai_chillDestination>
		</building>
		<costList>
			<WNA_Concrete>4</WNA_Concrete>
		</costList>
        <researchPrerequisites Inherit="false">
            <li>WNA_BaseDefense</li>
        </researchPrerequisites>
		<comps>
			<li Class="WNA.ThingCompProp.CompThingSelfHeal">
			  	<ticksPerHeal>27</ticksPerHeal>
				<healPercent>10</healPercent>
			</li>
			<li Class="WNA.ThingCompProp.CompDamageLimit">
			  	<damageLimit>1</damageLimit>
			</li>
		</comps>
	</ThingDef>

</Defs>