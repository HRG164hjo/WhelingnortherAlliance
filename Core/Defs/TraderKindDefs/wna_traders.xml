<Defs>
    <TraderKindDef ParentName="WNA_TraderKindBase">
        <defName>WNA_Trader_Settlement</defName>
        <label>whelingnorther branch storage</label>
        <stockGenerators>
            <li Class="StockGenerator_Animals">
                <tradeTagsSell>
                    <li>WNATradeTag_Animal</li>
                </tradeTagsSell>
                <tradeTagsBuy>
                    <li>AnimalCommon</li>
                    <li>AnimalDryad</li>
                    <li>AnimalExotic</li>
                    <li>AnimalFarm</li>
                    <li>AnimalFighter</li>
                    <li>AnimalInsect</li>
                    <li>AnimalPet</li>
                    <li>AnimalUncommon</li>
                    <li>WNATradeTag_Animal</li>
                </tradeTagsBuy>
                <kindCountRange>1~2</kindCountRange>
                <countRange>8~10</countRange>
            </li>
            <li Class="StockGenerator_Techprints">
                <countChances>
                    <li>
                        <count>5</count>
                        <chance>1</chance>
                    </li>
                </countChances>
            </li>
        </stockGenerators>
    </TraderKindDef>
