<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RecipeDef>
        <defName>WNARecipeTo_WNA_Medikit</defName>
        <label>administer medicine pack</label>
        <description>Administer a WNA medicine pack.</description>
        <workerClass>Recipe_AdministerUsableItem</workerClass>
        <jobString>Administering medicine pack.</jobString>
        <targetsBodyPart>false</targetsBodyPart>
        <anesthetize>false</anesthetize>
        <isViolation>false</isViolation>
        <workAmount>60</workAmount>
        <dontShowIfAnyIngredientMissing>true</dontShowIfAnyIngredientMissing>
        <surgeryOutcomeEffect IsNull="True"/>
		<recipeUsers>
			<li>WNA_WNThan</li>
			<li>WNA_Human</li>
			<li>Human</li>
			<li MayRequire="ludeon.rimworld.anomaly">CreepJoiner</li>
		</recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>WNA_Medikit</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>WNA_Medikit</li>
            </thingDefs>
        </fixedIngredientFilter>
    </RecipeDef>

    <RecipeDef ParentName="WNA_SurgeryBase">
        <defName>WNA_Inspection</defName>
        <label>psychic inspection</label>
        <description>Inspect target with one's psychic power, faster and no medicine is required. This technique requires basic level of intelligence.</description>
        <workerClass>WNA.Recipe.Inspection</workerClass>
        <jobString>Inspecting TargetA.</jobString>
        <hideBodyPartNames>true</hideBodyPartNames>
        <targetsBodyPart>false</targetsBodyPart>
        <uiIconPath>UI/Icons/SurgicalInspection</uiIconPath>
		<recipeUsers>
			<li>WNA_WNThan</li>
			<li>WNA_Human</li>
			<li>Human</li>
			<li MayRequire="ludeon.rimworld.anomaly">CreepJoiner</li>
		</recipeUsers>
        <skillRequirements>
            <Intellectual>4</Intellectual>
        </skillRequirements>
        <workAmount>60</workAmount>
        <researchPrerequisite>WNA_BasicRite</researchPrerequisite>
    </RecipeDef>
    <RecipeDef ParentName="WNA_SurgeryBase">
        <defName>WNA_Hemodialysis</defName>
        <label>blood dialysis</label>
        <description>Use specially formulated solution to perform blood dialysis on the patient, in order to extract toxic substances accumulated in the patient's body.</description>
        <workerClass>Recipe_RemoveHediff</workerClass>
        <jobString>Performing blood dialysis on TargetA.</jobString>
        <hideBodyPartNames>true</hideBodyPartNames>
        <targetsBodyPart>false</targetsBodyPart>
        <workAmount>30000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Neutroamine</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Neutroamine</li>
            </thingDefs>
        </fixedIngredientFilter>
        <removesHediff>WNA_ScrapBuildup</removesHediff>
    </RecipeDef>

    <RecipeDef ParentName="WNA_SurgeryBase">
        <defName>WNA_VoidDive</defName>
        <label>void dive</label>
        <description>force a human into deep coma and turn them into a anchor. the victim must be sober to face the void, thus voidsteel can be drained through them while they're struggling to awake, to escape, or doing anything else, before their consciousness finally fades.</description>
        <workerClass>Recipe_AddHediff</workerClass>
        <jobString>Forcing void dive on TargetA.</jobString>
        <hideBodyPartNames>true</hideBodyPartNames>
        <targetsBodyPart>false</targetsBodyPart>
        <workAmount>300</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Neutroamine</li>
                    </thingDefs>
                </filter>
                <count>2</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Neutroamine</li>
            </thingDefs>
        </fixedIngredientFilter>
        <addsHediff>WNA_VoidDiver</addsHediff>
    </RecipeDef>

    <RecipeDef ParentName="WNA_SurgeryBase">
        <defName>WNA_BrainHarvest</defName>
        <label>harvest brain</label>
        <description>harvest brain.</description>
        <workerClass>WNA.Recipe.TargetedHarvest</workerClass>
        <jobString>harvesting TargetA.</jobString>
        <hideBodyPartNames>true</hideBodyPartNames>
        <isViolation>true</isViolation>
        <workAmount>2500</workAmount>
        <deathOnFailedSurgeryChance>1</deathOnFailedSurgeryChance>
        <targetsBodyPart>true</targetsBodyPart>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>WNA_Methanol</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Neutroamine</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>WNA_Methanol</li>
                <li>Neutroamine</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <WNA_PreservedBrain>1</WNA_PreservedBrain>
        </products>
        <appliedOnFixedBodyParts>
            <li>Brain</li>
        </appliedOnFixedBodyParts>
    </RecipeDef>
