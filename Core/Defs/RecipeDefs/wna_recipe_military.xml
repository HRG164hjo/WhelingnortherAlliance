    <RecipeDef ParentName="WNA_RecipeBase">
        <defName>WNA_MakeAntigrainWarhead</defName>
        <label>make antigrain warhead</label>
        <description>Mix uranium and unstable particles and put the mixture into a plasteel shell to make an antigrain warhead.</description>
        <jobString>making antigrain warhead.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>75</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>WNA_ParticleVial</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Plasteel</li>
                <li>Uranium</li>
                <li>WNA_ParticleVial</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <Shell_AntigrainWarhead>5</Shell_AntigrainWarhead>
        </products>
        <workAmount>1500</workAmount>
        <recipeUsers>
	        <li>FabricationBench</li>
	    </recipeUsers>
        <researchPrerequisite>WNA_MilitaryMaterial</researchPrerequisite>
    </RecipeDef>

