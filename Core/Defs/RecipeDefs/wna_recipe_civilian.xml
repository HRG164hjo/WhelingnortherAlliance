    <RecipeDef ParentName="WNA_RecipeBase">
        <defName>WNA_MakeUraniumBattery</defName>
        <label>make vitalized uranium battery</label>
        <description>make an unstable powersource out from uranium.</description>
        <jobString>making vitalized uranium battery.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>40</count>
            </li>
            <li>
                <filter>
                    <categories>
                        <li>StoneBlocks</li>
                    </categories>
                </filter>
                <count>30</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>50</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>WNA_ParticleVial</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
                <li>Uranium</li>
                <li>WNA_ParticleVial</li>
            </thingDefs>
            <categories>
                <li>StoneBlocks</li>
            </categories>
        </fixedIngredientFilter>
        <products>
            <WNA_UraniumBattery>1</WNA_UraniumBattery>
        </products>
        <workAmount>800</workAmount>
        <recipeUsers>
	        <li>FabricationBench</li>
	    </recipeUsers>
        <researchPrerequisite/>
    </RecipeDef>

