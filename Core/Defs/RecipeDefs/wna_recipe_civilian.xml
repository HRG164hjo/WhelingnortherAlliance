    <RecipeDef ParentName="WNA_RecipeBase">
        <defName>WNA_MakeCinnabar</defName>
        <label>make cinnabar</label>
        <description>use mercury and sulphur to make cinnabar.</description>
        <jobString>making cinnabar.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>WNA_Sulphur</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>WNA_MercuryVial</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>WNA_Sulphur</li>
                <li>WNA_MercuryVial</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <WNA_Cinnabar>5</WNA_Cinnabar>
        </products>
        <workAmount>400</workAmount>
        <recipeUsers>
	        <li>DrugLab</li>
	    </recipeUsers>
        <researchPrerequisite/>
    </RecipeDef>
