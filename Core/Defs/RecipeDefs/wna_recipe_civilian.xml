<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- civil -->
    <RecipeDef ParentName="WNA_RecipeBase">
        <defName>WNA_HeatStoneChunk</defName>
        <label>heat stone chunk</label>
        <description>treat stone with strong heat to gain resources.</description>
        <jobString>heating a stone chunk.</jobString>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>StoneChunks</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>StoneChunks</li>
            </categories>
        </fixedIngredientFilter>
        <products>
            <WNA_Quicklime>15</WNA_Quicklime>
            <WNA_MercuryVial>2</WNA_MercuryVial>
            <WNA_HalogenVial>1</WNA_HalogenVial>
        </products>
        <workAmount>450</workAmount>
        <recipeUsers>
	        <li>ElectricSmelter</li>
	        <li>ElectricCrematorium</li>
	    </recipeUsers>
        <researchPrerequisite/>
    </RecipeDef>
    <RecipeDef ParentName="WNA_RecipeBase">
