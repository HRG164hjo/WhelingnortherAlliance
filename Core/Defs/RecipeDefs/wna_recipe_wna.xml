<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- wna -->
     <RecipeDef ParentName="WNA_RecipeBase">
        <defName>WNA_MakeDarkAsbestos</defName>
        <label>make dark asbestos</label>
        <description>transform stony materials into dark asbestos.</description>
        <jobString>making dark asbestos.</jobString>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>StoneChunks</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>StoneChunks</li>
            </categories>
        </fixedIngredientFilter>
        <products>
            <WNA_DarkAsbestos>75</WNA_DarkAsbestos>
        </products>
        <workAmount>200</workAmount>
        <recipeUsers>
	        <li>WNA_Ritespot</li>
	    </recipeUsers>
        <researchPrerequisite/>
    </RecipeDef>
