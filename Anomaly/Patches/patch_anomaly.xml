<?xml version="1.0" encoding="utf-8" ?>
<Patch>    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/TraderKindDef[@Name="WNA_TraderKindBase"]/stockGenerators</xpath>
        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>HarbingerSeed</thingDef>
                <price>VeryCheap</price>
                <countRange>100000~200000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Shard</thingDef>
                <price>VeryCheap</price>
                <countRange>300000~500000</countRange>
            </li>
            <li Class="StockGenerator_Tag">
                <tradeTag>Serum</tradeTag>
                <thingDefCountRange>2~2</thingDefCountRange>
                <countRange>100000~200000</countRange>
                <price>VeryCheap</price>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>GoldenCube</thingDef>
                <price>VeryCheap</price>
                <countRange>1200~1200</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>RevenantSpine</thingDef>
                <price>VeryCheap</price>
                <countRange>1000~1000</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>MonolithFragment</thingDef>
                <price>VeryCheap</price>
                <countRange>2000~2000</countRange>
            </li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="Bioferrite"]</xpath>
        <value>
            <deepCommonality>0.5</deepCommonality>
            <deepCountPerPortion>45</deepCountPerPortion>
            <deepLumpSizeRange>7~8</deepLumpSizeRange>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="WNA_WNThan" or defName="WNA_Human"]/statBases</xpath>
        <value>
            <MinimumContainmentStrength>10000</MinimumContainmentStrength>
            <EntityStudyRate>10000</EntityStudyRate>
            <StudyEfficiency>100</StudyEfficiency>
            <ActivitySuppressionRate>10000</ActivitySuppressionRate>
            <PsychicRitualQualityOffset>1</PsychicRitualQualityOffset>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/TerrainDef[defName="Voidmetal"]</xpath>
        <value>
            <costList>
                <WNA_Voidsteel>4</WNA_Voidsteel>
            </costList>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>Defs/TerrainDef[defName="Voidmetal"]/statBases</xpath>
        <value>
            <ContainmentStrength>60</ContainmentStrength>
        </value>
    </Operation>
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/TerrainDef[defName="Voidmetal"]/designationCategory</xpath>
    </Operation>

    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="VoidmetalWall"]</xpath>
        <value>
            <costList>
                <WNA_Voidsteel>4</WNA_Voidsteel>
            </costList>
            <designationCategory>Structure</designationCategory>
        </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
	    <xpath>Defs/ThingDef[defName="VoidmetalWall"]/building</xpath>
        <value>
            <building>
                <isInert>true</isInert>
                <repairable>false</repairable>
                <claimable>true</claimable>
                <deconstructible>true</deconstructible>
                <isTargetable>false</isTargetable>
                <supportsWallAttachments>true</supportsWallAttachments>
            </building>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationRemove">
	    <xpath>Defs/StatDef[defName="ActivitySuppressionRate"]/statFactors</xpath>
    </Operation>

    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThingDef[defName="ShardBeacon"]</xpath>
        <value>
            <useHitPoints>false</useHitPoints>
        </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
	    <xpath>Defs/ThingDef[defName="ShardBeacon"]/comps/li[@Class="CompProperties_Facility"]</xpath>
        <value>
            <li Class="CompProperties_Facility">
                <statOffsets>
                    <PsychicRitualQuality>0.25</PsychicRitualQuality>
                </statOffsets>
                <maxSimultaneous>4</maxSimultaneous>
                <maxDistance>10</maxDistance>
            </li>
        </value>
    </Operation>
    
    <Operation Class="PatchOperationAdd">
	    <xpath>Defs/ThoughtDef[defName="AteTwistedMeat" or defName="VoidClosed" or defName="Chimera" or defName="PsychicRitualGuilt" or defName="VoidCuriosity" or defName="VoidCuriositySatisfied" or defName="BecameGhoul" or defName="SpokeToDisturbing" or defName="GhoulNegative" or defName="GhoulPositive" or defName="GrayPall" or defName="UnnaturalDarkness" or defName="HateChantDrone" or defName="HarbingerScream" or defName="SwallowedByDarkness" or defName="Voidsight" or defName="SpokeToDisturbingMood" or defName="DeadlifeDust" or defName="DeathRefusalSickness"]</xpath>
        <value>
            <nullifyingPrecepts MayRequire="ludeon.rimworld.ideology">
                <li>WNA_P_Proselyte</li>
            </nullifyingPrecepts>
        </value>
    </Operation>
</Patch>