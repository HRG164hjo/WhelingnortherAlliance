<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="GravshipComponentBase">
        <defName>WNA_GravliteManifold</defName>
        <label>gravlite manifold</label>
        <description>A piece of advanced grav tech with a minified built-in self-maintenance system, can distort gravity and able to harvest stress energy from distorted gravitational field. A single facility is enough to maintain a medium sized orbital outpost.</description>
        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <texPath>buildings/wna_gravengine/wna_gravengine</texPath>
            <drawSize>3</drawSize>
        </graphicData>
		<designationCategory>WNA_MainCategory</designationCategory>
        <thingClass>Building_GravEngine</thingClass>
        <drawerType>RealtimeOnly</drawerType>
        <tickerType>Normal</tickerType>
		<useHitPoints>false</useHitPoints>
        <stealable>false</stealable>
        <size>(3,3)</size>
        <passability>PassThroughOnly</passability>
        <pathCost>0</pathCost>
        <canOverlapZones>false</canOverlapZones>
        <castEdgeShadows>true</castEdgeShadows>
        <fillPercent>0.6</fillPercent>
		<resourcesFractionWhenDeconstructed>1</resourcesFractionWhenDeconstructed>
        <holdsRoof>true</holdsRoof>
        <statBases>
            <Mass>99</Mass>
            <Flammability>0</Flammability>
            <Beauty>0</Beauty>
            <WorkToBuild>4000</WorkToBuild>
            <GravshipRange>500</GravshipRange>
            <SubstructureSupport>125660</SubstructureSupport>
        </statBases>
        <costList>
            <GravlitePanel>180</GravlitePanel>
            <Plasteel>160</Plasteel>
            <ComponentSpacer>18</ComponentSpacer>
            <Gravcore>4</Gravcore>
        </costList>
        <building>
            <ai_neverTrashThis>true</ai_neverTrashThis>
            <relatedTerrain>
                <li>WNA_FocusFloor</li>
            </relatedTerrain>
        </building>
        <researchPrerequisites>
            <li>WNA_AdvancedArsenal</li>
        </researchPrerequisites>
        <minifiedDef Inherits="false" IsNull="true"/>
        <thingCategories Inherits="false" IsNull="true"/>
        <comps>
            <li Class="CompProperties_SubstructureFootprint">
                <radius>200</radius>
            </li>
            <li Class="CompProperties_AffectedByFacilities">
                <linkableFacilities>
                    <li>GravFieldExtender</li>
                    <li>PilotConsole</li>
                    <li>FuelOptimizer</li>
                    <li>SignalJammer</li>
                    <li>PilotSubpersonaCore</li>
                    <li>GravshipShieldGenerator</li>
                    <li>WNA_UraniumFuelTank</li>
                    <li>WNA_ReinforcedThruster</li>
                    <li>ChemfuelTank</li>
                    <li>LargeChemfuelTank</li>
                    <li>SmallThruster</li>
                    <li>LargeThruster</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_FleshChemfuelTank</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_FleshLargeChemfuelTank</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_VoidGravFieldExtender</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_VoidThruster</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_Grav_WarpedObelisk_Mutator</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_Grav_WarpedObelisk_Duplicator</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_Grav_WarpedObelisk_Abductor</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_Grav_WarpedObelisk_Blood</li>
			        <li MayRequire="HaiLuan.VoidUniverse">UV_Grav_WarpedObelisk_UnnaturalDarkness</li>
                </linkableFacilities>
            </li>
            <li Class="CompProperties_Power">
                <compClass>CompPowerPlant</compClass>
                <basePowerConsumption>-1005000</basePowerConsumption>
                <transmitsPower>true</transmitsPower>
            </li>
        </comps>
    </ThingDef>

    <ThingDef ParentName="WNA_FurnitureBase">
        <defName>WNA_UraniumFuelTank</defName>
        <label>uranium fuel tank</label>
        <description>TODO</description>
        <graphicData>
            <texPath>buildings/misc/wna_fueltank</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>2</drawSize>
        </graphicData>
        <size>(2,2)</size>
        <statBases>
            <Mass>40</Mass>
            <Flammability>0</Flammability>
            <Beauty>0</Beauty>
            <WorkToBuild>4000</WorkToBuild>
        </statBases>
        <costList>
            <Steel>100</Steel>
            <Plasteel>40</Plasteel>
        </costList>
        <researchPrerequisites>
            <li>WNA_AdvancedArsenal</li>
        </researchPrerequisites>
        <tickerType>Normal</tickerType>
        <placeWorkers>
            <li>PlaceWorker_OnSubstructure</li>
        </placeWorkers>
        <comps>
            <li Class="CompProperties_GravshipFacility">
                <providesFuel>true</providesFuel>
                <componentTypeDef>FuelStorage</componentTypeDef>
                <requiresLOS>false</requiresLOS>
                <showMaxSimultaneous>false</showMaxSimultaneous>
                <maxSimultaneous>9999</maxSimultaneous>
                <maxDistance>500</maxDistance>
            </li>
            <li Class="CompProperties_Refuelable">
                <fuelCapacity>10000</fuelCapacity>
                <fuelMultiplier>1000</fuelMultiplier>
                <fuelFilter>
                    <thingDefs>
                        <li>WNA_UraniumBattery</li>
                    </thingDefs>
                </fuelFilter>
                <fuelLabel>vitalized uranium battery</fuelLabel>
                <fuelGizmoLabel>vitalized uranium battery</fuelGizmoLabel>
                <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
                <autoRefuelPercent>1</autoRefuelPercent>
                <showFuelGizmo>true</showFuelGizmo>
                <drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
                <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
                <canEjectFuel>true</canEjectFuel>
                <drawFuelGaugeInMap>false</drawFuelGaugeInMap>
            </li>
        </comps>
    </ThingDef>

    <ThingDef ParentName="WNA_FurnitureBase">
        <defName>WNA_ReinforcedThruster</defName>
        <label>reinforced thruster</label>
        <description>TODO</description>
        <graphicData>
            <graphicClass>Graphic_Multi</graphicClass>
            <texPath>Things/Building/Ship/ShipEngine</texPath>
            <drawSize>(2.85,2.15)</drawSize>
            <color>(74,74,74)</color>
        </graphicData>
        <size>(2,2)</size>
        <statBases>
            <Mass>50</Mass>
            <Flammability>0</Flammability>
            <Beauty>0</Beauty>
            <WorkToBuild>4000</WorkToBuild>
        </statBases>
        <costList>
            <Steel>100</Steel>
            <Plasteel>40</Plasteel>
        </costList>
        <researchPrerequisites>
            <li>WNA_AdvancedArsenal</li>
        </researchPrerequisites>
        <rotatable>true</rotatable>
        <tickerType>Rare</tickerType>
        <passability>Standable</passability>
        <blockWind>true</blockWind>
        <fillPercent>1</fillPercent>
        <holdsRoof>true</holdsRoof>
        <canOverlapZones>false</canOverlapZones>
        <blockLight>true</blockLight>
        <building>
            <isAirtight>true</isAirtight>
            <isInert>true</isInert>
        </building>
        <placeWorkers>
            <li>PlaceWorker_GravshipThruster</li>
            <li>PlaceWorker_OnSubstructure</li>
        </placeWorkers>
        <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
        <drawPlaceWorkersWhileInstallBlueprintSelected>true</drawPlaceWorkersWhileInstallBlueprintSelected>
        <comps>
            <li Class="CompProperties_GravshipThruster">
                <statOffsets>
                    <GravshipRange>250</GravshipRange>
                </statOffsets>
                <componentTypeDef>Thruster</componentTypeDef>
                <exclusionAreaSize>(0, 0, 0)</exclusionAreaSize>
                <exclusionAreaOffset>(0, 0, 0)</exclusionAreaOffset>
                <directionInfluence>30</directionInfluence>
                <flameSize>4.0</flameSize>
                <flameOffsetsPerDirection>
                    <li>(0, 0, 1.0)</li>
                    <li>(0, 0, 0.5)</li>
                    <li>(0, 0, 0.5)</li>
                    <li>(0, 0, 0.5)</li>
                </flameOffsetsPerDirection>
                <flameShaderType>MoteGlow</flameShaderType>
                <flameShaderParameters>
                    <_MainTex>/Things/Mote/LargeThruster_Burn</_MainTex>
                </flameShaderParameters>
                <requiresLOS>false</requiresLOS>
                <maxSimultaneous>8</maxSimultaneous>
                <maxDistance>500</maxDistance>
                <exhaustSettings>
                    <exhaustFleckDef>GravshipThrusterExhaust</exhaustFleckDef>
                    <emissionsPerSecond>40.0</emissionsPerSecond>
                    <spawnRadiusRange>0.0~0.5</spawnRadiusRange>
                    <velocity>(0.0,0.0,-20.0)</velocity>
                    <velocityRotationRange>-10.0~10.0</velocityRotationRange>
                    <velocityMultiplierRange>0.8~1.2</velocityMultiplierRange>
                    <rotationOverTimeRange>-180.0~180.0</rotationOverTimeRange>
                    <scaleRange>3.0~5.0</scaleRange>
                </exhaustSettings>
            </li>
            <li Class="CompProperties_Breakdownable" />
        </comps>
    </ThingDef>

</Defs>